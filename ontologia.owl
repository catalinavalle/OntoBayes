<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns="http://example.org/ontologia#"
         xml:base="http://example.org/ontologia#">

    <owl:Ontology rdf:about="http://example.org/ontologia#"/>

    <owl:Class rdf:about="http://example.org/ontologia#Factor"/>
    <owl:Class rdf:about="http://example.org/ontologia#Condicion"/>
    <owl:Class rdf:about="http://example.org/ontologia#Sintoma"/>

    <owl:ObjectProperty rdf:about="http://example.org/ontologia#causa">
        <rdfs:domain rdf:resource="http://example.org/ontologia#Factor"/>
        <rdfs:range rdf:resource="http://example.org/ontologia#Condicion"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/ontologia#provoca">
        <rdfs:domain rdf:resource="http://example.org/ontologia#Condicion"/>
        <rdfs:range rdf:resource="http://example.org/ontologia#Sintoma"/>
    </owl:ObjectProperty>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#consumo_de_tabaco">
        <rdf:type rdf:resource="http://example.org/ontologia#Factor"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#consumo_de_alcohol">
        <rdf:type rdf:resource="http://example.org/ontologia#Factor"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#infeccion_por_vph">
        <rdf:type rdf:resource="http://example.org/ontologia#Factor"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#cancer_oral">
        <rdf:type rdf:resource="http://example.org/ontologia#Condicion"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#ulcera_persistente">
        <rdf:type rdf:resource="http://example.org/ontologia#Sintoma"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#dolor_oral_persistente">
        <rdf:type rdf:resource="http://example.org/ontologia#Sintoma"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#sangrado_oral">
        <rdf:type rdf:resource="http://example.org/ontologia#Sintoma"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#dificultad_para_tragar">
        <rdf:type rdf:resource="http://example.org/ontologia#Sintoma"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#lesion_exofitica">
        <rdf:type rdf:resource="http://example.org/ontologia#Sintoma"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#leucoplasia">
        <rdf:type rdf:resource="http://example.org/ontologia#Sintoma"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/ontologia#eritroplasia">
        <rdf:type rdf:resource="http://example.org/ontologia#Sintoma"/>
    </owl:NamedIndividual>

    <rdf:Description rdf:about="http://example.org/ontologia#consumo_de_tabaco">
        <causa rdf:resource="http://example.org/ontologia#cancer_oral"/>
    </rdf:Description>

    <rdf:Description rdf:about="http://example.org/ontologia#consumo_de_alcohol">
        <causa rdf:resource="http://example.org/ontologia#cancer_oral"/>
    </rdf:Description>

    <rdf:Description rdf:about="http://example.org/ontologia#infeccion_por_vph">
        <causa rdf:resource="http://example.org/ontologia#cancer_oral"/>
    </rdf:Description>

    <rdf:Description rdf:about="http://example.org/ontologia#cancer_oral">
        <provoca rdf:resource="http://example.org/ontologia#ulcera_persistente"/>
        <provoca rdf:resource="http://example.org/ontologia#dolor_oral_persistente"/>
        <provoca rdf:resource="http://example.org/ontologia#sangrado_oral"/>
        <provoca rdf:resource="http://example.org/ontologia#dificultad_para_tragar"/>
        <provoca rdf:resource="http://example.org/ontologia#lesion_exofitica"/>
        <provoca rdf:resource="http://example.org/ontologia#leucoplasia"/>
        <provoca rdf:resource="http://example.org/ontologia#eritroplasia"/>
    </rdf:Description>

</rdf:RDF>